{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.insertSpaces": true
  },
  "eslint.options": {
    "extensions": [
      ".js",
      ".ts"
    ]
  },
  "eslint.validate": [
    "javascript"
  ],
  "cSpell.words": [
    "autosync",
    "dbaeumer",
    "Dsonar",
    "ilovepixelart",
    "jsonpatch",
    "lcov",
    "linebreak",
    "nargs",
    "nosql",
    "npmignore",
    "parens",
    "sonarcloud",
    "sonarsource",
    "swcrc",
    "virtuals"
  ]
}